//TEST

//OR
let a = 5;
let b = 5;

let result =
  a > b || a >= b ? "a is greater than equal to b" : "b is greater than a";
console.log("Is a > b or b > a? I predict a is greater than qual to b");
console.log(result);

//AND
b = 10;
result = a < b && a != b? "a is not equal and greater than b" : "A EQUAL AND LESS THAN B";
console.log("Is a > b or b > a? I predict a is not equal and greater than b");
console.log(result);
